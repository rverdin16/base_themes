header.page-header.header-classic
	// RD Navbar
	.rd-navbar-wrap
		nav.rd-navbar(data-layout='rd-navbar-fixed', data-sm-layout='rd-navbar-fixed', data-md-layout='rd-navbar-fullwidth', data-md-device-layout='rd-navbar-fixed', data-lg-layout='rd-navbar-fullwidth', data-lg-device-layout="rd-navbar-fullwidth",  data-md-stick-up-offset="73px", data-lg-stick-up-offset="93px" data-stick-up='true' data-sm-stick-up='true' data-md-stick-up='true' data-lg-stick-up='true')
			.rd-navbar-collapse-toggle(data-rd-navbar-toggle='.rd-navbar-collapse'): span

			.rd-navbar-aside-outer
				.rd-navbar-panel
					// RD Navbar Toggle
					button.rd-navbar-toggle(data-rd-navbar-toggle='.rd-navbar-nav-wrap')
						span
					// RD Navbar Brand
					.rd-navbar-brand
						a.brand-name(href='index.html')
							+png(config.logoDefault, "desktop-element")
							+png(config.logoGray2, "mobile-element")

				.rd-navbar-aside.rd-navbar-collapse
