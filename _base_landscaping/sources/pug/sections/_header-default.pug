											a(href='tel:#') 1 (800) 123 1234
										.p.tel
											a(href='tel:#') 1 (800) 123 1235

							li
								.unit.unit-middle.unit-spacing-icon.unit-horizontal
									.unit__left
										span.icon.icon-md.icon-primary-dark.fa-home
									.unit__body
										a.text-middle.address(href='contacts.html') 227 Marion Street,
											br
											| Columbia, SC 29201

							li
								.unit.unit-middle.unit-spacing-icon.unit-horizontal
									.unit__left
										span.icon.icon-md.icon-primary-dark.fa-clock-o
									.unit__body
										.p Mon - Sat 8.00 - 18.00
										.p Sunday CLOSED

						+widget-list([
							{
								"icon": "fa fa-facebook",
								"href": "#"
							},
							{
								"icon": "fa fa-google-plus",
