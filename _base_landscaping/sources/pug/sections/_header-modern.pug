header.page-header.header-modern
	// RD Navbar
	.rd-navbar-wrap
		nav.rd-navbar(data-layout='rd-navbar-fixed', data-sm-layout='rd-navbar-fixed', data-md-layout='rd-navbar-fullwidth', data-md-device-layout='rd-navbar-fixed', data-lg-layout='rd-navbar-static', data-lg-device-layout="rd-navbar-static",  data-md-stick-up-offset="60px", data-lg-stick-up-offset="33px" data-stick-up='true' data-sm-stick-up='true' data-md-stick-up='true' data-lg-stick-up='true')

			// RD Navbar Panel
			.rd-navbar-panel
				// RD Navbar Toggle
				button.rd-navbar-toggle(data-rd-navbar-toggle='.rd-navbar-nav-wrap')
					span
				// RD Navbar Brand
				.rd-navbar-brand
					a.brand-name(href='index.html')
						+png(config.logoGray2, "mobile-element")

			.shell
				.rd-navbar-inner.rd-navbar-nav-wrap
					.rd-navbar-nav-wrap-item.rd-navbar-nav-wrap-left
						+nav( 0, 4 )

					// RD Navbar Brand
					.rd-navbar-brand
						a.brand-name(href='index.html')
							+png(config.logoGray, "desktop-element")
							+png('logo-1-43x37', "desktop-element brand-position")

					.rd-navbar-nav-wrap-item.rd-navbar-nav-wrap-right
						+nav( 4, 8 )